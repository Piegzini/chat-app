{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": "warn"
  }
}
